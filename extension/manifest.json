{
    "name": "Hamii",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "hamster.png",
        "default_title": "Hamii"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png"
    }
}